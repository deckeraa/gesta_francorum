        <PAGEOBJECT XPOS="136.001" YPOS="5048.02" OwnPage="11" ItemID="495801906" PTYPE="4" WIDTH="522" HEIGHT="324.521031847134" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L522 0 L522 324.521 L0 324.521 L0 0 Z" copath="M0 0 L522 0 L522 324.521 L0 324.521 L0 0 Z" gXpos="136.001" gYpos="5048.02" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Ipsīque Chrīstiānī nēquiter dēdūcēbant"/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT CH=" sē, quia palātia urbis sternēbant et ārdēbant, et auferēbant plumbum quō"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT CH=" ecclēsiae erant coopertae et vēndēbant Graecīs. Unde imperātor īrātus est iussitque eōs trānsmeāre Brāchium."/>
                <para PARENT="Latin"/>
                <ITEXT CH="Postquam trānsfretāvērunt, nōn cessābant agere omnia mala, combūrentēs et dēvastantēs domōs et ecclēsiās. Tandem pervēnērunt Nīcomēdīam, ubi dīvīsī sunt Lombardī et Longobardī, et Alamannī ā Francīs, quia Francī tumēbant superbiā. Ēlēgērunt Lombardī et Longobardī seniōrem super sē, cui"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT CH=" nōmen Rainoldus, Alamannī similiter. Et intrāvērunt in Romāniam et per quattuor diēs iērunt ultrā Nicēnam urbem invēnēruntque quoddam castrum cui nōmen Exerogorgō, quod erat vacuum"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT CH=" gente. Et apprehendērunt illud, in quā invēnērunt satis frūmentī"/>
                <ITEXT FEATURES="inherit superscript" CH="5"/>
                <ITEXT CH=" et vīnī et carnis, et omnium bonōrum abundantiam."/>
                <para PARENT="Latin"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="136.001" YPOS="5372.54103184713" OwnPage="11" ItemID="496292962" PTYPE="4" WIDTH="522" HEIGHT="79.4904458598767" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L522 0 L522 79.4904 L0 79.4904 L0 0 Z" copath="M0 0 L522 0 L522 79.4904 L0 79.4904 L0 0 Z" gXpos="136.001" gYpos="5372.54103184713" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="The Christians themselves were conducting themselves wickedly."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="And they were carrying off the lead with which the churches were covered."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Whose name was Rainoldus."/>
                <ITEXT CH=" The dative "/>
                <ITEXT FONT="DejaVu Serif Italic" FEATURES="inherit" CH="cui"/>
                <ITEXT CH=" is used idiomatically here."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Which was empty of people"/>
                <ITEXT CH=". The ablative of separation is used here."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="5"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="In which they found enough corn and wine and meat"/>
                <ITEXT CH=". "/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Satis"/>
                <ITEXT CH=" is often used with the genitive."/>
                <trail PARENT="footnotes"/>
            </StoryText>
        </PAGEOBJECT>

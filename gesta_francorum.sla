        <PAGEOBJECT XPOS="136.001" YPOS="14200.053" OwnPage="33" ItemID="1733293311" PTYPE="4" WIDTH="522" HEIGHT="296.252732484076" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L522 0 L522 296.253 L0 296.253 L0 0 Z" copath="M0 0 L522 0 L522 296.253 L0 296.253 L0 0 Z" gXpos="136.001" gYpos="14200.053" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Quod audiēns Tancredus rediit retrō, et prōiectus in flūmen natandō pervēnit ad aliōs, et duo mīlia mīsērunt sē in flūmen sequendō Tancredum. Tandem invēnērunt Turcopūlōs et Pinzinacōs dīmicantēs cum nostrīs. Quōs repente"/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT CH=" fortiter invāsērunt, et prūdenter eōs superāvērunt. Et apprehendērunt plūrēs ex illīs, et dūxērunt illōs ligātōs ante dominī Boamundī praesentiam, quibus ait ipse, 'Quārē, miserī, occīditis gentem Chrīstī et meam? Ego cum vestrō imperātōre nūllam altercātiōnem habeō. Quī respondērunt: 'Nōs nequīmus"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT CH=" aliud agere; in rogā imperātōris locātī sumus, et quicquid nōbīs imperat nōs oportet implēre. Quōs Boamundus impūnītōs"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT CH=" permīsit abīre. Hoc bellum factum est in quārtā fēriā"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT CH=", quae est caput ieiūniī."/>
                <para PARENT="Latin"/>
                <ITEXT CH="Per omnia benedictus Deus. Āmēn. "/>
                <para PARENT="Latin"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="136.001" YPOS="14496.3057324841" OwnPage="33" ItemID="1733988463" PTYPE="4" WIDTH="522" HEIGHT="107.312101910808" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L522 0 L522 107.312 L0 107.312 L0 0 Z" copath="M0 0 L522 0 L522 107.312 L0 107.312 L0 0 Z" gXpos="136.001" gYpos="14496.3057324841" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Whom they (Tancred's army) suddenly strongly attacked, and prudently overpowered them."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="We are unable to do otherwise; we are in the pay of the emperor and whatever he commands us we have to fulfill."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Whom Bohemond allowed to leave, unpunished"/>
                <ITEXT CH=". Perhaps this was due to Bohemond having a spirit of clemency; perhaps it was due to political reasons relating to the Byzantine emperor."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT CH="This battle occurred on Ash Wednesday. From Hagenmeyer (my translation): The Easter festival was on April 5th 1097, therefore Ash Wednesday the 18th of February."/>
                <trail PARENT="footnotes"/>
            </StoryText>
        </PAGEOBJECT>

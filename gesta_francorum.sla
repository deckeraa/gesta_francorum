        <PAGEOBJECT XPOS="136.001" YPOS="16901.2356687898" OwnPage="39" ItemID="1748628703" PTYPE="4" WIDTH="522" HEIGHT="81.6305732484107" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L522 0 L522 81.6306 L0 81.6306 L0 0 Z" copath="M0 0 L522 0 L522 81.6306 L0 81.6306 L0 0 Z" gXpos="136.001" gYpos="16901.2356687898" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="And he left to speak with the emperor in Constantinople."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="And he commanded his men."/>
                <ITEXT CH=" The verb "/>
                <ITEXT FONT="DejaVu Serif Italic" CH="imperāre"/>
                <ITEXT CH=" takes the dative rather than the accusative."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="'When we are, in a disciplined manner, approaching the city, I will go first,' and who led with him a few soldiers."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="And seeing the pilgrims (the crusaders) to buy food, he talked amongst them, that they might leave from the road, and then might conduct the people to where they might more happily live."/>
                <trail PARENT="footnotes"/>

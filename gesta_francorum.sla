        <PAGEOBJECT XPOS="136.001" YPOS="4216.017" OwnPage="9" ItemID="1686187583" PTYPE="4" WIDTH="522" HEIGHT="286.409382165605" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L522 0 L522 286.409 L0 286.409 L0 0 Z" copath="M0 0 L522 0 L522 286.409 L0 286.409 L0 0 Z" gXpos="136.001" gYpos="4216.017" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="Fēcērunt dēnique Gallī trēs partēs"/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT CH=". Ūna pars Francōrum in Hungāriae intrāvit regiōnem, scīlicet Petrus Heremīta, et dux Godefridus, et Balduīnus frāter eius, et Balduīnus comes dē Monte. Istī potentissimī mīlitēs et aliī plūrēs quōs ignōrō"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT CH=" vēnērunt per viam quam iamdūdum Karolus Magnus mīrificus rēx Franciae aptārī fēcit usque Cōnstantīnopolim."/>
                <para PARENT="Latin"/>
                <ITEXT CH="Petrus vērō suprādictus prīmus vēnit Cōnstantīnopolim in kalendīs Augustī et cum eō maxima gēns Alamannōrum. Illīc invēnit Lombardōs et Longobardōs et aliōs plūrēs congregātōs, quibus imperātor iusserat darī mercātum, sīcuti erat in cīvitāte, dīxitque illīs 'Nōlīte"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT CH=" trānsmeāre Brāchium, dōnec veniat maxima Chrīstiānōrum virtūs, quoniam vōs tantī nōn estis, ut cum Turcīs praeliārī valeātis."/>
                <para PARENT="Latin"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="136.001" YPOS="4502.42638216561" OwnPage="9" ItemID="1683166543" PTYPE="4" WIDTH="522" HEIGHT="118.59061783439" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L522 0 L522 118.591 L0 118.591 L0 0 Z" copath="M0 0 L522 0 L522 118.591 L0 118.591 L0 0 Z" gXpos="136.001" gYpos="4502.42638216561" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Finally the Gauls made three parties. The three parties are three groups of crusaders who all took the same path to Constantinople. TODO identify the parties."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="These very powerful solides are many others (which I do not know) came through the road which long ago Charlemagne, the wonderful king of France, had made to be prepared all the way to Constantinople."/>
                <ITEXT CH=" The first person "/>
                <ITEXT FONT="DejaVu Serif Italic" CH="ignōrō"/>
                <ITEXT CH=" is used because this text is a first-hand account from a crusader. TODO include a map of this road, maybe https://en.wikipedia.org/wiki/File:Map_of_First_Crusade.png"/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Don't cross over the Bosporus, until the full (maximum) strength of Christian (soldiers) comes, because you are not so great, that you can prevail in battle with the Turks. "/>
                <trail PARENT="footnotes"/>
            </StoryText>
        </PAGEOBJECT>

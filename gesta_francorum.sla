                <ITEXT FONT="DejaVu Serif Italic" CH="pulsre"/>
                <ITEXT CH=" means the same thing as "/>
                <ITEXT FONT="DejaVu Serif Italic" CH="rogre"/>
                <ITEXT CH=" or "/>

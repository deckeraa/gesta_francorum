        <PAGEOBJECT XPOS="136.001" YPOS="2552.011" OwnPage="5" ItemID="485310738" PTYPE="4" WIDTH="522" HEIGHT="390.817025477707" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L522 0 L522 390.817 L0 390.817 L0 0 Z" copath="M0 0 L522 0 L522 390.817 L0 390.817 L0 0 Z" gXpos="136.001" gYpos="2552.011" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <para PARENT="Chapter Heading"/>
                <ITEXT CH="Liber 1"/>
                <para PARENT="Chapter Heading"/>
                <para PARENT="Chapter Heading"/>
                <ITEXT CH="Cum iam appropinquāsset"/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT CH=" ille terminus quem dominus Iēsus cotīdiē suīs dēmōnstrat fidēlibus, speciāliter in ēvangeliō"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT CH=" dīcēns: &quot;Sī quis vult post mē venīre, abneget sēmetipsum et tollat crucem suam et sequātur mē&quot; facta"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT CH=" est igitur mōtiō valida per ūniversās Galliārum regiōnēs, ut sī"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT CH=" aliquis Deum studiōsē pūrōque corde et mente sequī dēsīderāret, atque post ipsum crucem fidēliter baiulāre vellet"/>
                <ITEXT FEATURES="inherit superscript" CH="5"/>
                <ITEXT CH=", nōn pigritārētur"/>
                <ITEXT FEATURES="inherit superscript" CH="6"/>
                <ITEXT CH=" Sānctī Sepulchrī viam celerius arripere. Apostolicus namque Rōmānae sēdis ultrā montānās"/>
                <ITEXT FEATURES="inherit superscript" CH="7"/>
                <ITEXT CH=" partēs quantōcius profectus est cum suīs archiepiscopīs, epīscopīs, abbātibus, et presbytērīs, coepitque subtīliter sermōcinārī et praedīcāre, dīcēns, ut sī quis animam suam salvam facere vellet, nōn dubitāret humiliter viam incipere Dominī, ac sī dēnāriōrum eī dēesset"/>
                <ITEXT FEATURES="inherit superscript" CH="8"/>
                <ITEXT CH=" cōpia, dīvīna eī satis daret misericordia."/>
                <para PARENT="Latin"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="136.001" YPOS="2942.82802547771" OwnPage="5" ItemID="485971202" PTYPE="4" WIDTH="522" HEIGHT="236.427121019113" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L522 0 L522 236.427 L0 236.427 L0 0 Z" copath="M0 0 L522 0 L522 236.427 L0 236.427 L0 0 Z" gXpos="136.001" gYpos="2942.82802547771" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="That manner (of life) having drawn near, which Jesus every day pointed out to his faithful (disciples)."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT CH="The verse referenced is Matthew 16:24."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Therefore a strong movement occured through every region of Gallia."/>
                <para PARENT="footnotes"/>
                <ITEXT FONT="DejaVu Serif Italic" FEATURES="inherit superscript" CH="4"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="That if anyone was eagerly desiring to follow God with pure heart and min"/>
                <ITEXT CH="d."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="5"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="And wanted to faithfully carry his cross after him (Jesus)."/>
                <ITEXT CH=" The subjunctive "/>
                <ITEXT FONT="DejaVu Serif Italic" CH="vellet"/>
                <ITEXT CH=" is used because the statement is conditional. "/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Ipsum"/>
                <ITEXT CH=" is used as a more intense version of "/>
                <ITEXT FONT="DejaVu Serif Italic" CH="illum"/>
                <ITEXT CH="."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="6"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="He might not tarry (delay) to more quickly seize the road of the Holy Sepulchre."/>
                <ITEXT CH=" The Church of the Holy Sepulchre is located in Jerusalem and claims to be located on the spot where Christ was buried and resurrected. It was a popular pilgrimage destination, but the pilgrimage had been made very dangerous due to Muslim occupation."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="7"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Indeed the Apostle of the Roman seat (the Pope) across the mountains as quickly as possible set out with his archbishops, bishops, abbots, and elders, and began to precisely (eloquently, cleverly) converse and to preach."/>
                <ITEXT CH=" The montains mentioned are the Alps, which separate Gallia from Italy. This may be referring to the Council of Clermont, which took place November 17-27 1095, in which Pope Urban II set forth the idea of Crusade."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="8"/>
                <ITEXT CH="And if supply of money might be lacking to him, the divine mercy would give enough (sustenance) to him."/>
                <trail PARENT="footnotes"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="766.001" YPOS="2552.011" OwnPage="6" ItemID="486009506" PTYPE="4" WIDTH="522" HEIGHT="720" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="2" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L522 0 L522 720 L0 720 L0 0 Z" copath="M0 0 L522 0 L522 720 L0 720 L0 0 Z" gXpos="766.001" gYpos="2552.011" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="abbātibus: abbots; plural masculine ablative from abbās, abbātis"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="abneget: let him deny; 3rd person singular present active subjunctive from abnegō, abnegāre, abnegāvī, abnegātus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="ac: and"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="aliquis: someone"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="animam: soul, mind, life; singular feminine accusative from anima, animae"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="apostolicus: the Pope (medieval usage), bishop, apostle; singular masculine nominative from apostolicus, apostolica, apostolicum"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="appropinquāvisset: it (might) have had approached; 3rd person singular pluperfect active subjunctive from appropinquō, appropinquāre, appropinquāvī, appropinquātus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="archiepiscopīs: archbishops; plural masculine ablative from archiepiscopus, archiepiscopī"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="arripere: to take hold of, to snatch, to seize; present active infinitive from arripiō, arripere, arripuī, arreptus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="atque: and"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="bāiulāre: to carry, to bear; present active infinitive from bāiulō, bāiulāre, bāiulāvī, bāiulātus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="celerius: more quickly; comparative adverb from celer, celeris -e, celerior -or -us, celerrimus -a -um"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="coepit: he started; 3rd person singular present active from coepio, coepere, coepi, coeptus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="cōpia: supply; singular feminine nominative from cōpia, cōpiae"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="corde: heart; singular neuter ablative from cor, cordis"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="cotīdiē: daily"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="crucem: cross; singular feminine accusative from crux, crucis"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="cum: with"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="daret: it may give; 3rd person singular imperfect active subjunctive from do, dare, dedi, datus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="dēesset: it may lack; 3rd person singular imperfect active subjunctive from dēsum, dēesse, dēfuī, dēfutūrus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="dēmōnstrat: he shows, points out; 3rd person singular present active from dēmōnstrō, dēmōnstrāre, dēmōnstrāvī, dēmōnstrātus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="dēnāriōrum: of denariī (a denarius is a type of coin)"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="dēsīderāret: he may desire; 3rd person singular imperfect active subjunctive from dēsīderō, dēsīderāre, dēsīderāvī, dēsīderātus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="Deum: God; singular masculine accusative from Deus, Dei"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="dīcēns: saying; singular nominative present participle from dīcō, dīcere, dīxī, dictus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="dīvīna: divine; singular feminine nominative from dīvīnus, dīvīna -um, dīvīnior -or -us, dīvīnissimus -a -um"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="Dominī: of the lord; singular masculine genitive from dominus, dominī"/>
                <para PARENT="Vocab"/>
